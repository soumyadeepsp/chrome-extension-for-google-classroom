{
    "manifest_version" : 2,
    "name" : "Wikit",
    "version" : "1.0",
    "description" : "This is a simple extension which is used to search wikipedia on the go.",
    "icons" : {
        "128" : "icon128.png",
        "48" : "icon48.png",
        "16" : "icon16.png"
    },
    "background" : {
        "scripts" : ["eventPage.js"],
        "persistent" : false
    },
    "permissions" : [
        "tabs",
        "contextMenus"
    ]
}